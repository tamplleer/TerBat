<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tamplleer.terbat.SelectaLevelActivity">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/select3"
        android:layout_width="400dp"
        android:layout_height="200dp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="201dp">

        <ToggleButton
            android:id="@+id/bigmap_toggle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textOn="BIG on"
            android:textOff="BIG off"
            android:checked="false"
            android:onClick="onBIGmap"
            android:layout_marginRight="265dp"
            android:layout_marginTop="76dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/toggleButton"
            android:layout_marginLeft="8dp" />

        <ToggleButton
            android:id="@+id/smallmap_toggle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textOn="SMALL on"
            android:textOff="SMALL off"
            android:checked="true"
            android:onClick="onSMALLmap"
            android:layout_marginLeft="37dp"
            app:layout_constraintLeft_toLeftOf="@+id/select3"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="28dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/bigmap_toggle" />

        <ToggleButton
            android:id="@+id/pve_toggle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textOn="PVE on"
            android:textOff="PVE off"
            android:checked="true"
            android:onClick="onPvE"
            android:layout_marginRight="47dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.058" />

        <Button
            android:id="@+id/return3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:onClick="return3"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:text="return"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.941" />



        <ToggleButton
            android:id="@+id/pvp_toggle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textOn="PVP on"
            android:textOff="PVP off"
            android:checked="false"
            android:onClick="onPvP"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.058"
            app:layout_constraintRight_toLeftOf="@+id/pve_toggle"
            android:layout_marginRight="8dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.987" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/select1"
        android:layout_width="400dp"
        android:layout_height="200dp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="-1dp">

        <Button
            android:id="@+id/boss"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="39dp"
            android:text="boss"
            android:onClick="boss"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.958" />

        <Button
            android:id="@+id/education"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="education"
            android:onClick="education"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="76dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintRight_toLeftOf="@+id/boss"
            android:layout_marginRight="8dp"
            app:layout_constraintLeft_toRightOf="@+id/patient"
            android:layout_marginLeft="8dp" />

        <Button
            android:id="@+id/patient"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="8dp"
            android:text="patient"
            android:onClick="patient"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/button9"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" />
    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/exitSelLvl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Exit"
        android:onClick="Exit"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="-1dp" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/select2"
        android:layout_width="400dp"
        android:layout_height="200dp"
        tools:layout_editor_absoluteX="-8dp"
        tools:layout_editor_absoluteY="399dp">

        <Button
            android:id="@+id/button11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button"
            tools:layout_editor_absoluteX="269dp"
            tools:layout_editor_absoluteY="351dp" />

        <Button
            android:id="@+id/button13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button"
            tools:layout_editor_absoluteX="33dp"
            tools:layout_editor_absoluteY="345dp" />

        <Button
            android:id="@+id/button12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:text="Button"
            app:layout_constraintLeft_toRightOf="@+id/button13"
            tools:layout_editor_absoluteY="349dp" />

        <Button
            android:id="@+id/button10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:text="Button"
            app:layout_constraintLeft_toRightOf="@+id/button9"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteY="93dp" />

        <Button
            android:id="@+id/button9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="93dp"
            android:text="Button"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="156dp" />

        <Button
            android:id="@+id/button8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="8dp"
            android:text="Button"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/button9"
            tools:layout_editor_absoluteY="93dp"
            app:layout_constraintHorizontal_bias="0.441" />
    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/StartGame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="StartGame"
        android:onClick="StartGame"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.982"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>
